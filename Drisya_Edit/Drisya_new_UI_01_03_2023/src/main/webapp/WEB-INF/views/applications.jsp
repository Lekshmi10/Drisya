                                    <input type="checkbox" class="toggle-btn input-ctrl"  id="add_ext_flag">
                                    <span class="toggleLabel"></span>
                                    <span class="btn-label">New</span>
                                </div>
                                <button class="add-menu" onclick="addMenu(this.id)" id="extmenu_add">Add Menu</button>
                            </div>
                        </div>
                    </div>
                    <!-- END -->

                </div>
            </div>
            <div class="page-container tab-body-homepage" id="money-transfer">
                <div class="template-card">
                    <h3>Money transfer</h3>
                    <hr>
                    <!-- START -->
                    <div class="menu-container">
                        <div class="menu-widget">
                            <ul id="money_items-list" class="moveable drag-menu-row">
                            </ul>
                        </div>
                        <div class="add-menu-widget">
                            <div class="add-menu-form">
                                <h3>Create menu</h3>
