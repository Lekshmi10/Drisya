        <div class="template-container">
            <div class="template-header">
                <button class="toggle-button">
                    <svg width="23" height="16.45" viewBox="0 0 23 16.45"><g transform="translate(-400 -942.55)"><path class="def" d="M21,18v2H3V18ZM6.95,3.55v9.9L2,8.5,6.95,3.55ZM21,11v2H12V11Zm0-7V6H12V4Z" transform="translate(402 939)" fill="#707070"/><path class="hov" d="M21,18v2H3V18ZM2.95,3.55v9.9L-2,8.5,2.95,3.55ZM21,11v2H12V11Zm0-7V6H12V4Z" transform="translate(402 939)" fill="#707070"/></g></svg>
                </button>
                <div class="template-ctrl">
                    <button class="ctrl-btn" id="application_publish">Save & Publish</button>
                </div>
                <div class="user-control">
                    <div class="user-head"><!--open-->
                        <img src="./resources/images/avatar-f.jpg" alt=""  onclick="signOut()">
                        <div class="user-name"><% out.print(session.getAttribute("uname")); %></div>
                        <input class="user-role" id="user-role" value="<% out.print(session.getAttribute("role")); %>"  style="display:none"></input>
                    </div>
                </div>
            </div>
             <div Id="signout" style="display:none;">
                <a href="./signout" class="signout-div">Signout</a>
            </div>
            <div class="nav-widget-primary">
                <ul>
                    <li><a href="#" class="nav-menu-list active" data-menu="applicarions">Applications</a></li>
                    <li><a href="#" class="nav-menu-list" data-menu="side-menu">Side Menu</a></li>
                    <li><a href="#" class="nav-menu-list" data-menu="external-link">External link</a></li>
                    <li><a href="#" class="nav-menu-list" data-menu="money-transfer">Money transfer</a></li>
                </ul>
            </div>
            <div class="page-container tab-body-homepage active" id="applicarions">
                <div class="menu-tab-row">
                    <div class="tab-nav-list tab-navigation active" data-reftab="left-template">Applications Left Menu</div>
                    <div class="tab-nav-list tab-navigation" data-reftab="right-template">Applications Right Menu</div>
                </div>
                <div class="application-tab-list active" id="left-template">
                    <div class="template-card">
                        <h3>Left pane</h3>
                        <hr>
                        <!-- START -->
                        <div class="menu-container">
                            <div class="menu-widget">
                                <ul id="left_pane_items-list" class="moveable drag-menu-row">
                                </ul>
                            </div>
                            <div class="add-menu-widget">
                                <div class="add-menu-form">
                                    <h3>Create menu</h3>
                                    <hr class="mb-18">
                                    <div class="input-row" id="left-text-add">
                                        <input type="text" class="input-ctrl" placeholder="Enter Text" id="left_title">
                                    </div>
                                    <div class="input-row" id="left-url-add">
                                        <input type="text" class="input-ctrl" placeholder="Enter Url" id="left_url">
                                    </div>
                                    <div class="togle-btn-row mb-15">
                                        <input type="checkbox" class="toggle-btn input-ctrl" id="add_left_flag">
                                        <span class="toggleLabel"></span>
                                        <span class="btn-label">New</span>
                                    </div>
                                    <button class="add-menu" onclick="addMenu(this.id)" id="left_add">Add Menu</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="application-tab-list" id="right-template">
                    <div class="template-card">
                        <h3>Right Pane</h3>
                        <hr>
                        <!-- START -->
                        <div class="menu-container">
                            <div class="menu-widget">
                                <ul id="right_pane_items-list" class="moveable drag-menu-row">
                                </ul>
                            </div>
                            <div class="add-menu-widget">
                                <div class="add-menu-form">
                                    <h3>Create menu</h3>
                                    <hr class="mb-18">
                                    <div class="input-row" id="right-text-add">
                                        <input type="text" class="input-ctrl" placeholder="Enter Text" id="right_title">
                                    </div>
                                    <div class="input-row" id="right-url-add">
                                        <input type="text" class="input-ctrl" placeholder="Enter Url" id="right_url">
                                    </div>
                                    <div class="togle-btn-row mb-15">
                                        <input type="checkbox" class="toggle-btn input-ctrl"  id="add_right_flag">
                                        <span class="toggleLabel"></span>
                                        <span class="btn-label">New</span>
                                    </div>
                                    <button class="add-menu" onclick="addMenu(this.id)" id="right_add">Add Menu</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="page-container tab-body-homepage" id="side-menu">
                <div class="template-card">
                    <h3>Side Menu</h3>
                    <hr>
                    <div class="menu-container">
                        <div class="menu-widget" id="sidemenu_widget">
                           <ul id="sidemenu_items-list" class="moveable drag-menu-row">
                              </ul>
                        </div>
                        <div class="add-menu-widget">
                            <div class="add-menu-form">
                                <h3>Create menu</h3>
                                <hr class="mb-18">
